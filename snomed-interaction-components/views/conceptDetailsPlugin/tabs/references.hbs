<br/>
<b><span style="padding-left: 10px;" class='i18n' data-i18n-id='i18n_concept_has_references'>{{i18n "i18n_concept_has_references" "This tab shows all concepts that are targeting the selected concept"}}.</span></b>
<br/><br/>
{{#each result.referencesByType}}
    <div style="margin-top: 10px;" class="panel panel-default">
        <div class="panel-heading">
            <h3 style="font-size: 12px" class="panel-title">
                <a style="text-decoration: inherit;" data-toggle="collapse" data-parent="#references-{{divElementId}}-accordion" href="#references-{{@index}}">
                    <span id="references-{{@index}}-span" class="references glyphicon glyphicon-{{#if_gr referencingConcepts.length 10}}chevron-right{{else}}chevron-down{{/if_gr}}"></span>
                </a>&nbsp;{{referenceType.fsn.term}} ({{referencingConcepts.length}})
            </h3>
        </div>
        <div id="references-{{@index}}" class="panel-collapse collapse {{#if_gr referencingConcepts.length 10}}{{else}}in{{/if_gr}}">
            <div class="panel-body">
                <table class="table table-hover table-bordered">
                    <thead>
                    <tr>
                        <th><span class='i18n' data-i18n-id='i18n_term'>Term</span></th>
                        <th><span data-i18n-id="i18n_conceptId" class="i18n">{{i18n "i18n_preferred_term" "Preferred Term"}}</span></th>
                        <th><span data-i18n-id="i18n_conceptId" class="i18n">{{i18n "i18n_conceptId" "Concept Id"}}</span></th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each referencingConcepts}}
                        <tr>
                            <td>
                                {{#if_eq definitionStatus "PRIMITIVE"}}
                                    <span class="reference-item badge alert-warning context-menu" style="cursor: pointer" data-fsn="{{fsn.term}}" data-preferred-term="{{pt.term}}" draggable='true' ondragstart="drag(event, '{{../divElementId}}')" data-module="{{moduleId}}" data-concept-id='{{conceptId}}' data-term='{{fsn.term}}'>&nbsp;&nbsp;</span>
                                {{else}}
                                    <span class="reference-item badge alert-warning context-menu" style="cursor: pointer" data-fsn="{{fsn.term}}" data-preferred-term="{{pt.term}}" draggable='true' ondragstart="drag(event, '{{../divElementId}}')" data-module="{{moduleId}}" data-concept-id='{{conceptId}}' data-term='{{fsn.term}}'>&equiv;</span>                                    
                                {{/if_eq}}
                                {{#hasCountryIcon moduleId}}
                                    <div class="phoca-flagbox" style="width:22px;height:22px">
                                        <span class="phoca-flag {{countryIcon moduleId}}"></span>
                                    </div>
                                {{/hasCountryIcon}}
                                <span style="cursor: pointer" class="reference-item" data-concept-id='{{conceptId}}'>{{fsn.term}}</span>
                            </td>
                            <td>
                                <span style="cursor: pointer" class="reference-item" data-concept-id='{{conceptId}}'>{{pt.term}}</span>
                            </td>
                            <td><span style="cursor: pointer" class="reference-item" data-concept-id='{{conceptId}}'>{{conceptId}}</span></td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{{/each}}

