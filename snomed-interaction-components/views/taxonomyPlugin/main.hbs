<div style='height:100%;margin: 5px; overflow:auto;' class='panel panel-default' id='{{divElementId}}-mainPanel'>
    <div ondrop="dropT(event, '{{divElementId}}')" ondragleave="removeHighlight();" ondragover="allowDrop(event)" class='panel-heading' id='{{divElementId}}-panelHeading'>
        <!--<button id='{{divElementId}}-ownMarker' class='btn btn-link btn-lg' style='padding: 2px; position: absolute;top: 1px;left: 0px;'><i class='glyphicon glyphicon-book'></i></button>-->
        <button id='{{divElementId}}-subscribersMarker' class='btn btn-link btn-lg' style='padding: 2px; position: absolute;top: 1px;left: 15px;'><i class='glyphicon glyphicon-bookmark'></i></button>
        <div class='row'>
            <div class='col-md-6' id='{{divElementId}}-panelTitle'>&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<strong><span class='i18n' data-i18n-id='i18n_taxonomy'>Taxonomy</span></strong></div>
            <div class='col-md-6 text-right'>
                <button id='{{divElementId}}-historyButton' class='btn btn-link history-button' style='padding:2px'><i class='glyphicon glyphicon-time'></i></button>
                <button id='{{divElementId}}-resetButton' class='btn btn-link' data-panel='{{divElementId}}' style='padding:2px'><i class='glyphicon glyphicon-repeat'></i></button>
                <!--<button id='{{divElementId}}-linkerButton' draggable="true" ondragstart="drag(event, '{{divElementId}}')" class='btn btn-link linker-button' data-panel='{{divElementId}}' style='padding:2px'><i class='glyphicon glyphicon-link'></i></button>-->
                <!--<button id='{{divElementId}}-configButton' class='btn btn-link' data-toggle='modal' style='padding:2px' data-target='#{{divElementId}}-configModal'><i class='glyphicon glyphicon-cog'></i></button>-->
                <button id='{{divElementId}}-collapseButton' class='btn btn-link' style='padding:2px'><i class='glyphicon glyphicon-resize-small'></i></button>
                <button id='{{divElementId}}-expandButton' class='btn btn-link' style='padding:2px'><i class='glyphicon glyphicon-resize-full'></i></button>
                <button id='{{divElementId}}-closeButton' class='btn btn-link' style='padding:2px'><i class='glyphicon glyphicon-remove'></i></button>
            </div>
        </div>
    </div>
    <div id='{{divElementId}}-taxonomyConfigBar' style='margin-bottom: 10px;'><nav class='navbar navbar-default' role='navigation' style='min-height: 28px;border-radius: 0px;border-bottom: 1px lightgray solid;'>
        <ul class='nav navbar-nav navbar-left'>
            <li class='dropdown' style='margin-bottom: 2px; margin-top: 2px;'>
                <a href='javascript:void(0);' class='dropdown-toggle' data-toggle='dropdown' style='padding-top: 2px; padding-bottom: 2px;'><span id='{{divElementId}}-txViewLabel'></span> <b class='caret'></b></a>
                <ul class='dropdown-menu' role='menu' style='float: none;'>
                    <li><button class='btn btn-link' id='{{divElementId}}-inferredViewButton'><span class='i18n' data-i18n-id='i18n_inferred_view'>Inferred view</span></button></li>
                    <li><button class='btn btn-link' id='{{divElementId}}-statedViewButton'><span class='i18n' data-i18n-id='i18n_stated_view'>Stated view</span></button></li>
                </ul>
            </li>
            <li class='dropdown' style='margin-bottom: 2px; margin-top: 2px;'>               
                <a href='javascript:void(0);' class='dropdown-toggle' data-toggle='dropdown' style='padding-top: 2px; padding-bottom: 2px;'><span id='{{divElementId}}-txLanguageSwitcherLabel'></span> <b class='caret'></b></a>
                <ul class='dropdown-menu' role='menu' style='float: none;' id='{{divElementId}}-txLanguageMenu'></ul>
            </li>
            <li class='dropdown' style='margin-bottom: 2px; margin-top: 2px;'>
                <a href='javascript:void(0);' class='dropdown-toggle' data-toggle='dropdown' style='padding-top: 2px; padding-bottom: 2px;'><span id='{{divElementId}}-txViewLabel2'></span> <b class='caret'></b></a>
                <ul class='dropdown-menu' role='menu' style='float: none;'>
                    <li><button class='btn btn-link' id='{{divElementId}}-descendantsCountTrue'><span class='i18n' data-i18n-id='i18n_on'>On</span></button></li>
                    <li><button class='btn btn-link' id='{{divElementId}}-descendantsCountFalse'><span class='i18n' data-i18n-id='i18n_off'>Off</span></button></li>
                </ul>
            </li>
        </ul>
    </nav></div>
    <div class="row" id="{{divElementId}}-selectedConceptSection" style="margin: 0px">
        <div class="col-md-11">
            <h4><span id="{{divElementId}}-selectedConceptTerm"></span></h4>
        </div>
        <div class="col-md-1">
            <button class="btn btn-link" style="padding-top: 2px; padding-bottom: 10px; padding-left: 0px; padding-right: 0px; outline: none">
                <i class="glyphicon glyphicon-remove pull-right" id="{{divElementId}}-clearConceptButton"></i>
            </button>
        </div>
    </div>
    <div ondrop="dropT(event, '{{divElementId}}')" ondragleave="removeHighlight();" ondragover="allowDrop(event)" class='panel-body' style='height:100%' id='{{divElementId}}-panelBody'>
    </div>
</div>
<div class='modal fade' id='{{divElementId}}-configModal'>
    <div class='modal-dialog'>
        <div class='modal-content'>
            <div class='modal-header'>
                <button type='button' class='close' data-dismiss='modal' aria-hidden='true'>&times;</button>
                <h4 class='modal-title'><span class='i18n' data-i18n-id='i18n_options'>Options</span> ({{divElementId}})</h4>
            </div>
            <div class='modal-body' id='{{divElementId}}-modal-body'>
                <p></p>
            </div>
            <div class='modal-footer'>
                <button type='button' class='btn btn-danger' data-dismiss='modal'><span class='i18n' data-i18n-id='i18n_cancel'>Cancel</span></button>
                <button id='{{divElementId}}-apply-button' type='button' class='btn btn-success' data-dismiss='modal'><span class='i18n' data-i18n-id='i18n_apply_changes'>Apply changes</span></button>
            </div>
        </div>
    </div>
</div>
